{
  "name": "@echi/ui",
  "version": "0.0.1",
  "main": "dist/mui.umd.js",
  "browser": "dist/mui.cjs.js",
  "module": "dist/mui.esm.js",
  "unpkg": "dist/mui.unpkg.min.js",
  "types": "types/mui.d.ts",
  "files": [
    "docs",
    "dist",
    "types"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "release": "npm run build:lib",
    "build:lib": "rimraf dist & npm run build:cjs & npm run build:umd & npm run build:esm & npm run build:unpkg",
    "build:cjs": "cross-env NODE_ENV=production rollup --config rollup.config.js --format cjs",
    "build:umd": "cross-env NODE_ENV=production rollup --config rollup.config.js --format umd",
    "build:esm": "cross-env NODE_ENV=production rollup --config rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config rollup.config.js --format iife",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "vue-cli-service lint --fix --ext .js,.ts,.vue src",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "element-ui": "^2.14.1",
    "lodash-es": "^4.17.20",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "autoprefixer": "^10.2.1",
    "babel-plugin-component": "^1.1.1",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^9.5.0",
    "minimist": "^1.2.5",
    "node-sass": "^4.12.0",
    "postcss": "^8.2.4",
    "postcss-csso": "^5.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-vue": "5.1.9",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.7.1"
  },
  "peerDependencies": {
    "element-ui": "^2.14.1",
    "postcss": "^8.2.3",
    "rollup-plugin-vue": "5.1.9",
    "vue": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "npm run lint",
      "git add"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
